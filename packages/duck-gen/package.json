{
  "$schema": "https://json.schemastore.org/package.json",
  "engines": {
    "bun": ">=1.3.5"
  },
  "author": "gentleduck",
  "bin": {
    "duck-gen": "./bin/duck-gen.js"
  },
  "bugs": {
    "url": "https://github.com/gentleeduck/duck-gen/issues"
  },
  "dependencies": {
    "chalk": "5.6.2",
    "ora": "^9.0.0",
    "ts-morph": "^27.0.2",
    "tsx": "^4.21.0",
    "typescript": "5.9.3",
    "zod": "4.2.1"
  },
  "description": "Type-safe API and message generator for TypeScript projects (NestJS-tested).",
  "devDependencies": {
    "@gentleduck/tsdown-config": "workspace:*",
    "@gentleduck/typescript-config": "workspace:*",
    "@types/node": "22.19.3",
    "tsdown": "0.18.3"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./generated": "./generated/index.d.ts",
    "./generated/*": "./generated/*",
    "./nestjs": "./generated/nestjs/index.d.ts",
    "./nestjs/*": "./generated/nestjs/*",
    "./schema": "./duck-gen.schema.json"
  },
  "files": [
    "bin",
    "dist",
    "generated",
    "duck-gen.schema.json",
    "README.md",
    "LICENSE"
  ],
  "homepage": "https://github.com/gentleeduck/duck-gen/tree/master/packages/duck-gen#readme",
  "keywords": [
    "gentleduck",
    "duck-gen",
    "codegen",
    "api",
    "routes",
    "nestjs",
    "typescript",
    "i18n"
  ],
  "license": "MIT",
  "name": "@gentleduck/gen",
  "types": "./dist/index.d.mts",
  "packageManager": "bun@1.3.5",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "directory": "packages/duck-gen",
    "type": "git",
    "url": "git+https://github.com/gentleeduck/duck-gen.git"
  },
  "scripts": {
    "build": "tsdown",
    "check-types": "tsc -p tsconfig.json --noEmit --pretty false --skipLibCheck",
    "dev:cli": "bun ./src/index.ts",
    "dev:types": "tsc -p tsconfig.json --noEmit --watch --preserveWatchOutput --pretty false --skipLibCheck",
    "prepack": "bun run build"
  },
  "type": "module",
  "version": "0.2.7"
}
