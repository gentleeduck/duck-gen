    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@gentleduck/typescript-config": "workspace:*",
    "@opennextjs/cloudflare": "^1.0.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "22.19.3",
    "@types/react": "^19.2.8",
